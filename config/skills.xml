<?xml version="1.0" encoding="UTF-8"?>
<edu.tigers.sumatra.skillsystem base="edu.tigers.sumatra.skillsystem">
    <driver>
        <AroundBallDriver>
            <driveSpeed4BallSpeed>0.8</driveSpeed4BallSpeed>
            <minFutureTime>0.7</minFutureTime>
            <securityDistance2Ball>100.0</securityDistance2Ball>
            <triangleAngle>0.3</triangleAngle>
        </AroundBallDriver>
        <KickBallDriver>
            <ballVelTolerance>0.5</ballVelTolerance>
            <distBehindBallHitTarget>10</distBehindBallHitTarget>
            <futureBallLookahead>0.1</futureBallLookahead>
            <posTolerance>50.0</posTolerance>
            <secDist2Ball>200.0</secDist2Ball>
            <triangleAngleOuter>0.7</triangleAngleOuter>
        </KickBallDriver>
        <KickSkillCalc>
            <chillAcc>2.0</chillAcc>
            <chillVel>1.5</chillVel>
            <distForEnablingDribbler>400.0</distForEnablingDribbler>
            <theirPenAreaOffset>300.0</theirPenAreaOffset>
        </KickSkillCalc>
        <MixedPathDriver>
            <changeToClosePathDriverThreshold>100.0</changeToClosePathDriverThreshold>
        </MixedPathDriver>
        <PathPointDriver>
            <distTolerance>100.0</distTolerance>
            <raySizeMax>150.0</raySizeMax>
            <raySizeMin>30.0</raySizeMin>
        </PathPointDriver>
        <PositionDriver>
            <maxDistance>10000.0</maxDistance>
            <maxDistance:TIGER_V3>10000</maxDistance:TIGER_V3>
        </PositionDriver>
        <PushBallDriver>
            <pushDistance>50.0</pushDistance>
        </PushBallDriver>
        <SplinePathDriver>
            <positionMoveLookAhead>0.3</positionMoveLookAhead>
            <positionMoveMode>POS_ON_TANGENT</positionMoveMode>
        </SplinePathDriver>
        <TrajPathDriver>
            <angleRemTimeMultiplier>5.0</angleRemTimeMultiplier>
            <maxAngleDist>4</maxAngleDist>
            <minAngleDist>0.3</minAngleDist>
            <positionMoveLookAhead>0.1</positionMoveLookAhead>
        </TrajPathDriver>
        <TrajPathDriverV2>
            <lowerRotationSpeed>1.0</lowerRotationSpeed>
            <lowerRotationTrajTime>0.5</lowerRotationTrajTime>
            <relativeVelLimit>false</relativeVelLimit>
            <upperRotationSpeed>10.0</upperRotationSpeed>
            <upperRotationTrajTime>2.0</upperRotationTrajTime>
        </TrajPathDriverV2>
    </driver>
    <skills>
        <AMoveSkill>
            <defaultMoveSkill>GLOBAL_POSITION</defaultMoveSkill>
            <emergencyBreakAcc>5</emergencyBreakAcc>
            <motorModelType>MATRIX</motorModelType>
        </AMoveSkill>
        <AMoveToSkill>
            <defaultPathDriver>MIXED_SPLINE_POS</defaultPathDriver>
            <moveToType>TRAJECTORY</moveToType>
        </AMoveToSkill>
        <AReceiveSkill>
            <minReceiverPositioningTime>0.2</minReceiverPositioningTime>
        </AReceiveSkill>
        <BlockSkill>
            <blockDecisionVelocity>0.1</blockDecisionVelocity>
            <chipDuration>3000</chipDuration>
            <deaccelerationOfBall>1.0</deaccelerationOfBall>
            <deaccelerationOfBall:GRSIM>1.0</deaccelerationOfBall:GRSIM>
            <distanceBallAttacker>500</distanceBallAttacker>
            <dribbleSpeed>5000</dribbleSpeed>
            <keeperReactsOnAttackerPosition>true</keeperReactsOnAttackerPosition>
        </BlockSkill>
        <CatchSkill>
            <angleWheel1>0.7853981633974483</angleWheel1>
            <angleWheel2>1.0471975511965976</angleWheel2>
            <MAX_ITERATIONS>20</MAX_ITERATIONS>
            <stepSize>90.0</stepSize>
            <timeLookAhead>0.5</timeLookAhead>
        </CatchSkill>
        <IdleSkill>
            <breakAcc>5</breakAcc>
            <feedbackFreqActive>120</feedbackFreqActive>
            <feedbackFreqIdle>5</feedbackFreqIdle>
            <updateRateIdle>20.0</updateRateIdle>
        </IdleSkill>
        <KickSkill>
            <brokenKickDelay>0.7</brokenKickDelay>
            <defDribbleSpeed>2000</defDribbleSpeed>
            <waitBallSpeedTol>2.0</waitBallSpeedTol>
            <workingKickDelay>0.0</workingKickDelay>
        </KickSkill>
        <MoveToSkill>
            <pathFilter>HERMITE_SPLINE</pathFilter>
            <pathFinder>ERRT</pathFinder>
        </MoveToSkill>
        <MoveToTrajSkill>
            <debug>false</debug>
        </MoveToTrajSkill>
        <MoveToV2Skill>
            <defaultPathDriver>MIXED_SPLINE_POS</defaultPathDriver>
            <pathFilter>HERMITE_SPLINE</pathFilter>
            <pathFinder>ERRT</pathFinder>
        </MoveToV2Skill>
        <PenaltyKeeperSkill>
            <moveSidewards>false</moveSidewards>
        </PenaltyKeeperSkill>
        <PenaltyShootSkill>
            <correctionDist>0.0</correctionDist>
            <dribbleSpeed>0</dribbleSpeed>
            <slowMoveDist>150.0</slowMoveDist>
            <stepTimeForSlowMove>100</stepTimeForSlowMove>
            <timeToShoot>50</timeToShoot>
        </PenaltyShootSkill>
        <PenaltyShootSkillNew>
            <dribblerSpeed>0.0</dribblerSpeed>
            <kickSpeed>8.0</kickSpeed>
            <rotationSpeed>2.0</rotationSpeed>
            <slowMoveDist>200.0</slowMoveDist>
            <slowMoveVel>0.1</slowMoveVel>
            <timeToShoot>100.0</timeToShoot>
        </PenaltyShootSkillNew>
        <ReceiverSkill>
            <ballVelMoveBorder>0.3</ballVelMoveBorder>
            <dribbleSpeed>3000</dribbleSpeed>
            <maxMoveBackSpeed>2.0</maxMoveBackSpeed>
            <safePassSpeed>2.0</safePassSpeed>
        </ReceiverSkill>
        <RedirectSkill>
            <safetyTime>0.3</safetyTime>
        </RedirectSkill>
        <test>
            <LatencyTestSkill>
                <maxMeasures>5</maxMeasures>
                <maxSamples>100</maxSamples>
                <moveDiff>500.0;0.0</moveDiff>
            </LatencyTestSkill>
        </test>
    </skills>
</edu.tigers.sumatra.skillsystem>
